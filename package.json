{
  "name": "aikgrid",
  "version": "0.2.4",
  "types": "./dist/main.d.ts",
  "files": [
    "dist"
  ],
  "main": "./dist/aik-grid.cjs",
  "module": "./dist/aik-grid.js",
  "exports": {
    ".": {
      "import": "./dist/aik-grid.js",
      "require": "./dist/aik-grid.umd.cjs"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && pnpm build:types && pnpm rename",
    "build:types": "vue-tsc  --skipLibCheck --skipDefaultLibCheck --project ./tsconfig.build.json --declaration --emitDeclarationOnly --outDir dist/",
    "preview": "vite preview",
    "rename": "renamer --find main.d.ts --replace aik-grid.d.ts ./dist/*",
    "test:unit": "vitest",
    "test:unit-api": "vitest $1",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@coleqiu/vue-drag-select": "^2.0.3",
    "pinia": "^2.0.35",
    "uuid": "^9.0.0",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "renamer": "^4.0.0",
    "@playwright/test": "^1.33.0",
    "@rollup/plugin-typescript": "^11.1.1",
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.0",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.16.3",
    "@types/uuid": "^9.0.1",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vitest/ui": "^0.31.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.3.2",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "jsdom": "^22.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "rollup-plugin-typescript2": "^0.34.1",
    "sass": "^1.62.1",
    "tslib": "^2.5.3",
    "typescript": "~5.0.4",
    "vite": "^4.3.4",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^0.31.0",
    "vue-tsc": "^1.6.4"
  }
}
